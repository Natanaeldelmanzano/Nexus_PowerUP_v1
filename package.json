{
  "name": "nexus-ai-studio",
  "version": "1.0.0",
  "description": "NEXUS AI Studio - Advanced Multimodal AI Development Environment",
  "main": "dist/index.js",
  "homepage": "https://nexus-ai-studio.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/nexus-ai-studio.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/nexus-ai-studio/issues"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist node_modules/.vite",
    "clean:all": "rimraf dist node_modules node_modules/.vite",
    "install:clean": "npm run clean:all && npm install",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:dist": "npm run build && electron-builder --publish=never",
    "electron:publish": "npm run build && electron-builder --publish=always",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "multimodal",
    "development",
    "ide",
    "code-editor",
    "react",
    "typescript",
    "vite",
    "electron",
    "gpt",
    "claude",
    "gemini",
    "deepseek"
  ],
  "author": {
    "name": "NEXUS AI Team",
    "email": "team@nexus-ai-studio.com",
    "url": "https://nexus-ai-studio.com"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-virtual": "^3.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "fuse.js": "^7.0.0",
    "highlight.js": "^11.9.0",
    "lucide-react": "^0.292.0",
    "monaco-editor": "^0.44.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.18.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "react-window": "^1.8.8",
    "recharts": "^2.8.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-syntax-highlighter": "^15.5.9",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "electron": "^27.0.4",
    "electron-builder": "^24.6.4",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^0.34.6",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:run"
    }
  },
  "build": {
    "appId": "com.nexus.ai-studio",
    "productName": "NEXUS AI Studio",
    "directories": {
      "output": "electron-dist"
    },
    "files": [
      "dist/**/*",
      "public/electron.js",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}